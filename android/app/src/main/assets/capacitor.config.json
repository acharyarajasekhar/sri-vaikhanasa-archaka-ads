{
  "appId": "net.srivaikhanasa.archakaads",
  "appName": "ArchakaAds",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "plugins": {
    "SplashScreen": {
      "androidScaleType": "CENTER_CROP",
      "launchAutoHide": false
    },
    "CapacitorFirebaseAuth": {
      "providers": [
        "phone"
      ],
      "languageCode": "en",
      "nativeAuth": false,
      "permissions": {}
    }
  },
  "cordova": {
    "preferences": {
      "NavigationBarBackgroundColor": "#000000",
      "NavigationBarLigth": "false"
    }
  },
  "server": {
    "url": "http://192.168.0.105:8100",
    "cleartext": true
  }
}
